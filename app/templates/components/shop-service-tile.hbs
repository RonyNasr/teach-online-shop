<div class="col-sm-6 col-md-4">
  <div class="thumbnail">
    <img src= {{service.photo}}> <br>
    <div class="caption">
      <h3>{{service.name}}</h3>
      <p>{{service.description}}</p>
      <p>{{service.price}}</p>
      <div class="add-to-cart">
        {{#link-to 'feedback' service.id}} <button class="btn btn-info" >Feedback</button> {{/link-to}}
        <button class="btn btn-warning" {{action 'addToCart' service}}>Add to Cart</button>
      </div>
    </div>
  </div>
</div>
